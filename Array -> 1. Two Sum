class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
    int temp1=0, temp2=0, count=0;
    for(int i=0; i<nums.size(); i++){
        for(int j=i+1; j<nums.size(); j++){
            if(nums[j]==(target - nums[i])){
                temp1=i;
                temp2=j;
                count++;
                break;
            }
        }
        if(count==1){break;}
    }
    return {temp1,temp2};    
    }
};

